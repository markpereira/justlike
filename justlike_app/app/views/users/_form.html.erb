<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address_1 %>
  <%= f.text_field :address_1 %>

  <%= f.label :address_2 %>
  <%= f.text_field :address_2 %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :postcode %>
  <%= f.text_field :postcode %>

  <%= f.label :country %>
  <%= f.text_field :country %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :image %>
  <%= f.url_field :image %>

  <%= f.submit %>
<% end %>
